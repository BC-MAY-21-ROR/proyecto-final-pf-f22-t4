<h1>Users Management</h1>
  <table class="table">
    <thead>
      <th>Name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Birthdate</th>
      <th>Address</th>
      <th>Role</th>
      <th>Actions</th>
    </thead> 
    <% @users.each do |user| %>
      <%= render user %>
    <% end %>
  </table>